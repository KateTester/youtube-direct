<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">

  <!-- BEGIN admin servlets -->
  <servlet>
    <servlet-name>Test</servlet-name>
    <servlet-class>com.google.yaw.admin.FullTextIndexer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Test</servlet-name>
    <url-pattern>/admin/test</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>GetAllSubmissions</servlet-name>
    <servlet-class>com.google.yaw.admin.GetAllSubmissions</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetAllSubmissions</servlet-name>
    <url-pattern>/admin/GetAllSubmissions</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>PersistAuthSubToken</servlet-name>
    <servlet-class>com.google.yaw.admin.PersistAuthSubToken</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PersistAuthSubToken</servlet-name>
    <url-pattern>/admin/PersistAuthSubToken</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>InsightDownloadRedirect</servlet-name>
    <servlet-class>com.google.yaw.admin.InsightDownloadRedirect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsightDownloadRedirect</servlet-name>
    <url-pattern>/admin/InsightDownloadRedirect</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>VideoDownloadRedirect</servlet-name>
    <servlet-class>com.google.yaw.admin.VideoDownloadRedirect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VideoDownloadRedirect</servlet-name>
    <url-pattern>/admin/VideoDownloadRedirect</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>GetAdminConfig</servlet-name>
    <servlet-class>com.google.yaw.admin.GetAdminConfig</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetAdminConfig</servlet-name>
    <url-pattern>/admin/GetAdminConfig</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>PersistAdminConfig</servlet-name>
    <servlet-class>com.google.yaw.admin.PersistAdminConfig</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PersistAdminConfig</servlet-name>
    <url-pattern>/admin/PersistAdminConfig</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SyncMetadata</servlet-name>
    <servlet-class>com.google.yaw.admin.SyncMetadata</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SyncMetadata</servlet-name>
    <url-pattern>/admin/SyncMetadata</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UpdateSubmission</servlet-name>
    <servlet-class>com.google.yaw.admin.UpdateSubmission</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateSubmission</servlet-name>
    <url-pattern>/admin/UpdateSubmission</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DeleteSubmission</servlet-name>
    <servlet-class>com.google.yaw.admin.DeleteSubmission</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteSubmission</servlet-name>
    <url-pattern>/admin/DeleteSubmission</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>NewAssignment</servlet-name>
    <servlet-class>com.google.yaw.admin.NewAssignment</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NewAssignment</servlet-name>
    <url-pattern>/admin/NewAssignment</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UpdateAssignment</servlet-name>
    <servlet-class>com.google.yaw.admin.UpdateAssignment</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateAssignment</servlet-name>
    <url-pattern>/admin/UpdateAssignment</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>GetAllAssignments</servlet-name>
    <servlet-class>com.google.yaw.admin.GetAllAssignments</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetAllAssignments</servlet-name>
    <url-pattern>/admin/GetAllAssignments</url-pattern>
  </servlet-mapping>
  
  <!-- END admin servlets -->
   
  <!-- BEGIN user-facing servlets -->

  <servlet>
    <servlet-name>ApprovedVideoFeed</servlet-name>
    <servlet-class>com.google.yaw.ApprovedVideoFeed</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ApprovedVideoFeed</servlet-name>
    <url-pattern>/ApprovedVideoFeed</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>MobileAuthSub</servlet-name>
    <servlet-class>com.google.yaw.mobile.MobileAuthSub</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MobileAuthSub</servlet-name>
    <url-pattern>/MobileAuthSub</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PersistMobileSubmission</servlet-name>
    <servlet-class>com.google.yaw.mobile.PersistMobileSubmission</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PersistMobileSubmission</servlet-name>
    <url-pattern>/PersistMobileSubmission</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SubmitExistingVideo</servlet-name>
    <servlet-class>com.google.yaw.SubmitExistingVideo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SubmitExistingVideo</servlet-name>
    <url-pattern>/SubmitExistingVideo</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UploadResponseHandler</servlet-name>
    <servlet-class>com.google.yaw.UploadResponseHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadResponseHandler</servlet-name>
    <url-pattern>/UploadResponseHandler</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>GetUploadToken</servlet-name>
    <servlet-class>com.google.yaw.GetUploadToken</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetUploadToken</servlet-name>
    <url-pattern>/GetUploadToken</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AuthsubHandler</servlet-name>
    <servlet-class>com.google.yaw.AuthSubHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AuthsubHandler</servlet-name>
    <url-pattern>/AuthsubHandler</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>LogoutHandler</servlet-name>
    <servlet-class>com.google.yaw.LogoutHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutHandler</servlet-name>
    <url-pattern>/LogoutHandler</url-pattern>
  </servlet-mapping>
  
  <!-- END user-facing servlets -->

  <!-- BEGIN user-facing JSPs -->
  
  <servlet>
    <servlet-name>embed</servlet-name>
    <jsp-file>/embed.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>embed</servlet-name>
    <url-pattern>/embed</url-pattern>
  </servlet-mapping>
  
  <!-- END user-facing JSPs -->
  
  <!-- BEGIN admin JSPs -->
  
  <servlet>
    <servlet-name>admin</servlet-name>
    <jsp-file>/admin/admin.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>admin</servlet-name>
    <url-pattern>/admin</url-pattern>
  </servlet-mapping>
  
  <!-- END admin JSPs -->	
	
  <security-constraint>
    <web-resource-collection>
      <url-pattern>/admin/*</url-pattern>
      <url-pattern>/admin</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <role-name>admin</role-name>
    </auth-constraint>
    <user-data-constraint>
      <transport-guarantee>CONFIDENTIAL</transport-guarantee>
    </user-data-constraint>
  </security-constraint>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
